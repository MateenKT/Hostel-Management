<!-- fragment: sap.ui.com.project1.fragment.MaintainData (replace existing) -->
<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:u="sap.ui.unified"
>
    <Dialog
        title="{i18n>FillDetails}"
        resizable="true"
        draggable="true"
        contentWidth="40rem"
    >
        <f:Form
            id="dynamicForm"
            editable="true"
            class="sapUiSmallMargin"
        >
            <f:layout>
                <f:ResponsiveGridLayout />
            </f:layout>
        </f:Form>
        <footer>
            <Toolbar>
                <ToolbarSpacer />
                <Button
                    id="MD_id_submitButoon"
                    text="Save"
                    press="MD_onSubmitButtonPress"
                />
                <Button
                    id="MD_id_saveButoon"
                    text="{i18n>save}"
                    press="MD_onUpdateButtonPress"
                />

                <Button
                    text="Cancel"
                    press="MD_onCancelButtonPress"
                />
            </Toolbar>
        </footer>
    </Dialog>
</c:FragmentDefinition>
